<div class="page-header">
  <h1>
    Occupancies
    <a href="/occupancies/new" class="btn btn-lg btn-success">New Occupancy</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Office</th>
        <th>Organization</th>
        <th>Staff count</th>
        <th>Actions</th>
      </tr>

      <% @occupancies.each do |occupancy| %>
      <tr>
        <td><%= occupancy.office.name %></td>
        <td><%= occupancy.organization.name %></td>
        <td><%= occupancy.staff_count %></td>
        <td>
          <a href="/occupancies/<%= occupancy.id %>" class="btn btn-primary">Show</a>
          <a href="/occupancies/<%= occupancy.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_occupancy/<%= occupancy.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
