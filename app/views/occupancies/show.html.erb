<div class="page-header">
  <h3>Occupancy of <%= @occupancy.organization.name %> in <%= @occupancy.office.name %> </h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Organization</dt>
      <dd><%= @occupancy.organization.name %></dd>

      <dt>Office</dt>
      <dd><%= @occupancy.office.name %></dd>

      <dt>Staff count</dt>
      <dd><%= @occupancy.staff_count %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/occupancies" class="btn btn-primary">
        Back
      </a>
      <a href="/occupancies/<%= @occupancy.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_occupancy/<%= @occupancy.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
