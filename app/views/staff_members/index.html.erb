<div class="page-header">
  <h1>
    Staff members
    <a href="/staff_members/new" class="btn btn-lg btn-success">New Staff member</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Telephone</th>
        <!--<th>Photo</th>-->
        <!--<th>Em contact name</th>-->
        <!--<th>Em contact email</th>-->
        <!--<th>Em contact telephone</th>-->
        <th>Organization</th>
        <th>Office</th>
        <th>Id</th>
        <th>Actions</th>
      </tr>

      <% @staff_members.each do |staff_member| %>
      <tr>
        <td><%= staff_member.name %></td>
        <td><%= staff_member.email %></td>
        <td><%= staff_member.telephone %></td>
        <!--<td><%= staff_member.photo %></td>-->
        <!--<td><%= staff_member.em_contact_name %></td>-->
        <!--<td><%= staff_member.em_contact_email %></td>-->
        <!--<td><%= staff_member.em_contact_telephone %></td>-->
        <td><%= staff_member.organization.name %></td>
        <td><%= staff_member.office.name %></td>
        <td><%= staff_member.id %></td>
        <td>
          <a href="/staff_members/<%= staff_member.id %>" class="btn btn-primary">Show</a>
          <a href="/staff_members/<%= staff_member.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_staff_member/<%= staff_member.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
